{
  "properties": {
    "autoswagger": {
      "additionalProperties": false,
      "nullable": true,
      "properties": {
        "apiKeyHeaders": {
          "default": [],
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "title": {
          "type": "string"
        },
        "apiType": {
          "default": "httpApi",
          "enum": ["http", "httpApi"],
          "type": "string"
        },
        "basePath": {
          "type": "string"
        },
        "host": {
          "type": "string"
        },
        "excludeStages": {
          "default": [],
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "generateSwaggerOnDeploy": {
          "default": true,
          "type": "boolean"
        },
        "lambdaAuthorizer": {
          "oneOf": [
            { "type": "string" },
            {
              "type": "object",
              "properties": {
                "name": { "type": "string" },
                "arn": { "type": "string" },
                "identitySource": { "type": "string" },
                "identityValidationExpression": { "type": "string" },
                "resultTtlInSeconds": { "type": ["integer", "string"] },
                "type": { "type": "string" }
              }
            },
            {
              "type": "object",
              "properties": {
                "name": { "type": "string" },
                "scopes": { "type": "array", "items": { "type": "string" } }
              },
              "required": ["name"]
            },
            {
              "type": "object",
              "properties": {
                "id": { "type": "string" },
                "scopes": { "type": "array", "items": { "type": "string" } }
              },
              "required": ["name"]
            }
          ]
        },
        "schemes": {
          "items": {
            "enum": ["http", "https", "ws", "wss"],
            "type": "string"
          },
          "type": "array"
        },
        "swaggerFiles": {
          "default": ["./src/types/api-types.d.ts"],
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "swaggerPath": {
          "default": "swagger",
          "type": "string"
        },
        "typefiles": {
          "default": [],
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "useStage": {
          "default": false,
          "type": "boolean"
        }
      },
      "required": [],
      "type": "object"
    }
  },
  "type": "object"
}
